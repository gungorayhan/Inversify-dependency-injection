{
    "compilerOptions": {
      "target": "es5",                        
      "module": "CommonJS",
      "strict": true,
      "lib": ["DOM", "ES6", "ESNext.AsyncIterable"],
      "sourceMap": true,
      "outDir": "dist",
      "rootDir": "./backend",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "strictPropertyInitialization": false,
      "forceConsistentCasingInFileNames": true,
      "noImplicitAny": false, 
      "skipLibCheck": true,
      "incremental": true,
      "typeRoots": ["./backend/types", "./node_modules/@types"],
      "types": ["node", "express"],
    },
    "include": ["backend/**/*.ts"],
    "exclude": ["node_modules"],
}